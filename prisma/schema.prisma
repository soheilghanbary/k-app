generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Device {
  id       String   @id @default(uuid())
  name     String?  @default("")
  type     String?  @default("")
  serial   String
  phone    String?  @default("")
  address  String?  @default("")
  expert   String?  @default("")
  seller   String?  @default("")
  date     DateTime @default(now())
  packages Json[]
}
